@model Sys_NLogMapping
<div class="space-10"></div>
<div class="col-sm-12">

    <dl class="dl-horizontal">
        <dt>应用</dt>
        <dd>@Model.Application</dd>
        <dt>Level</dt>
        <dd>@Model.Level</dd>
        <dt>时间</dt>
        <dd>@Model.LoggedFormat</dd>
        <dt>Message</dt>
        <dd>@Model.Message</dd>
        <dt>来源类</dt>
        <dd>@Model.Logger</dd>
        <dt>Callsite</dt>
        <dd>@Model.Callsite</dd>
        <dt>Exception</dt>
        <dd>@Model.Exception</dd>
    </dl>

</div>






